local Players = game:GetService('Players')
local player = Players.LocalPlayer
local playerGui = player:WaitForChild('PlayerGui')

local ScreenGui = Instance.new('ScreenGui')
ScreenGui.Name = 'GeneratedGUI'
ScreenGui.ResetOnSpawn = false
ScreenGui.Parent = playerGui

local TextButton_1 = Instance.new('TextButton')
TextButton_1.Name = 'TextButton'
TextButton_1.Size = UDim2.new(0.2000, 0, 0.0000, 50)
TextButton_1.Position = UDim2.new(0.8000, 0, 0.6742, 0)
TextButton_1.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
TextButton_1.BackgroundTransparency = 0.00
TextButton_1.Visible = true
TextButton_1.Font = Enum.Font.SourceSansSemibold
TextButton_1.Text = [[Noot! Noot!]]
TextButton_1.TextColor3 = Color3.fromRGB(255, 255, 255)
TextButton_1.TextSize = 16.0
TextButton_1.TextScaled = true
TextButton_1.TextWrapped = true
TextButton_1.TextXAlignment = Enum.TextXAlignment.Center
TextButton_1.TextYAlignment = Enum.TextYAlignment.Center
TextButton_1.Parent = ScreenGui


-- ====== INTERACTIVITY ======

-- Connect Button Actions
TextButton_1.MouseButton1Click:Connect(function()
    pcall(function()
    local lighting = game:GetService("Lighting")
    local players = game:GetService("Players")
    local workspace = game:GetService("Workspace")
    
    -- **1. Cambia il cielo**
    local sky = Instance.new("Sky")
    sky.SkyboxBk = "rbxassetid://382332426"
    sky.SkyboxDn = "rbxassetid://382332426"
    sky.SkyboxFt = "rbxassetid://382332426"
    sky.SkyboxLf = "rbxassetid://382332426"
    sky.SkyboxRt = "rbxassetid://382332426"
    sky.SkyboxUp = "rbxassetid://382332426"
    lighting:ClearAllChildren()
    sky.Parent = lighting
    
    -- **2. Spamma decal su tutti gli oggetti**
    local function spamDecals()
        for _, part in pairs(workspace:GetDescendants()) do
            if part:IsA("BasePart") then -- Applica decal su ogni blocco
                for _, face in pairs(Enum.NormalId:GetEnumItems()) do
                    local decal = Instance.new("Decal")
                    decal.Texture = "rbxassetid://382332426"
                    decal.Face = face
                    decal.Parent = part
                end
            end
        end
    end
    spawn(spamDecals)
    
    -- **3. Aggiunge particelle ai giocatori**
    local function addParticlesToPlayers()
        for _, player in pairs(players:GetPlayers()) do
            if player.Character then
                for _, limb in pairs(player.Character:GetDescendants()) do
                    if limb:IsA("BasePart") then
                        local particle = Instance.new("ParticleEmitter")
                        particle.Texture = "rbxassetid://382332426"
                        particle.Rate = 50 -- Numero ridotto per evitare lag
                        particle.Lifetime = NumberRange.new(5, 10)
                        particle.Speed = NumberRange.new(5, 10)
                        particle.Size = NumberSequence.new(2)
                        particle.Parent = limb
                    end
                end
            end
        end
    end
    spawn(addParticlesToPlayers)
    
    -- **4. Aggiunge una nebbia blu densa**
    lighting.FogEnd = 150 -- Visibilit√† ridotta ma non troppo
    lighting.FogStart = 30
    lighting.FogColor = Color3.fromRGB(0, 0, 255) -- Blu intenso
    
    -- **5. Riproduce il suono all'infinito**
   local noot = Instance.new("Sound", game.SoundService) noot.Volume = 0.1 noot.PlaybackSpeed = 0.155 noot.SoundId = "rbxassetid://114190526353443" for i = 1,6 do Instance.new("DistortionSoundEffect", noot) end Instance.new("PitchShiftSoundEffect", noot).Octave = 1.185 noot:Play()

       for _, part in pairs(workspace:GetDescendants()) do
            if part:IsA("BasePart") then 
                part.Anchored = false
            end
        end
    
    function getAll(obj)
    for i, v in pairs(obj:getChildren()) do
    if v:IsA("BasePart") then 
    v. BrickColor = BrickColor.new ()
    v.Anchored = false
    bv = Instance.new("BodyVelocity")
    bv.Parent = v
    bv.maxForce = Vector3.new(75,75,75)
    end
    getAll(v)
    end
    end
    getAll(workspace)
    game.Lighting.TimeOfDay = "07:00:00"
    game.Lighting.Ambient = Color3.new(1,1,1)
    sky = Instance.new("Sky")
    sky.Parent = game.Lighting
    sky.SkyboxBk = "http://www.roblox.com/asset/?id=0"
    sky.SkyboxDn = "http://www.roblox.com/asset/?id=0"
    sky.SkyboxFt = "http://www.roblox.com/asset/?id=0"
    sky.SkyboxLf = "http://www.roblox.com/asset/?id=0"
    sky.SkyboxRt = "http://www.roblox.com/asset/?id=0"
    sky.SkyboxUp = "http://www.roblox.com/asset/?id=0"

local script = Instance.new('Script', disco)
 
		local Lighting = game:GetService("Lighting")
		local ColorCorrection = Instance.new("ColorCorrectionEffect", Lighting)
 
		function zigzag(X) 
			return math.acos(math.cos(X*math.pi))/math.pi
		end
 
		Counter = 0
 
		Lighting.TimeOfDay = "12:00"
 
		while wait(0.1) do 
			ColorCorrection.TintColor = Color3.fromHSV(zigzag(Counter),1,1)
			Lighting.Ambient = Color3.fromHSV(zigzag(Counter),1,1)
			Counter += 0.01
		end

    end)
end)

